QListWidget {
    min-width: 120px;
    max-width: 120px;
    color: Black;
    background: #F5F5F5;
    outline: 0px;
}

QListWidget::Item:selected {
    background: lightGray;
    border-left: 5px solid red;
}

QListWidget::Item {
    width: 120px;
    height: 50px;
}

HistoryPanel:hover {
    background: rgb(52, 52, 52);
}

QTableView {
    text-align: center;
    selection-background-color: #44c767;
}

QTableView::horizontalHeader {
    resizeSection(0, 30);
}